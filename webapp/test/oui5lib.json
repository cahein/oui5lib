{
    "environment": "development",
    "availableLanguages": [ "en", "de" ],
    "defaultLanguage": "en",
    "logLevel": "ERROR",
    "mappingDirectory": "mapping",
    "validation": {
        "dateRegex": "^\\d{4}-\\d{2}-\\d{2}$",
        "timeRegex": "^\\d{2}:\\d{2}:\\d{2}$",
        "phoneRegex": "^0{2}[1-9][\\d]*$"
    },
    "defaultFormats": {
        "dateTimeDisplay": "MMM d, y, HH:mm:ss",
        "dateTimeValue": "YYYY-MM-dd HH:mm:ss",
        "dateDisplay": "short",
        "dateValue": "YYYY-MM-dd",
        "timeDisplay": "HH:mm",
        "timeValue": "HH:mm:ss"
    }
}
